{
  "name": "gatsby-starter-typescript-deluxe",
  "description": "A Gatsby starter with TypeScript, Storybook, Styled Components, Jest, and more.",
  "version": "0.0.4",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/gojutin/gatsby-starter-typescript-deluxe"
  },
  "bugs": {
    "url": "https://github.com/gojutin/gatsby-starter-typescript-deluxe/issues"
  },
  "scripts": {
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "format": "prettier --write src/**/*.{js,jsx,ts,tsx}",
    "lint": "eslint . --ext ts --ext tsx --ext js --ext jsx",
    "lint:watch": "esw -w . --ext ts --ext tsx --ext js --ext jsx",
    "lint:fix": "npm run lint --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "serve": "gatsby serve",
    "storybook": "start-storybook -p 6006",
    "clean": "gatsby clean",
    "build": "gatsby build",
    "build:storybook": "build-storybook",
    "update": "ncu -u"
  },
  "dependencies": {
    "@storybook/addon-controls": "^6.5.9",
    "@storybook/addon-essentials": "^6.5.9",
    "@storybook/addon-storysource": "^6.5.9",
    "framer-motion": "^2.9.4",
    "gatsby": "^4.0.0",
    "gatsby-image": "^2.4.21",
    "gatsby-link": "^4.16.0",
    "gatsby-page-utils": "^2.16.0",
    "gatsby-plugin-react-axe": "^0.5.0",
    "gatsby-source-drupal": "^5.17.1",
    "modern-css-reset": "^1.3.0",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet": "^6.1.0"
  },
  "devDependencies": {
    "@babel/core": "^7.12.3",
    "@storybook/addon-actions": "^6.5.9",
    "@storybook/addon-knobs": "^6.0.28",
    "@storybook/addon-links": "^6.0.28",
    "@storybook/addons": "^6.0.28",
    "@storybook/react": "^6.0.28",
    "@testing-library/jest-dom": "^5.11.5",
    "@testing-library/react": "^11.1.1",
    "@types/jest": "^26.0.15",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "babel-jest": "^26.6.3",
    "babel-loader": "^8.2.0",
    "babel-preset-gatsby": "^0.5.16",
    "babel-preset-react-app": "^10.0.0",
    "eslint": "^7.13.0",
    "eslint-config-gojutin": "0.1.5",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-immutable": "^1.0.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.2.0",
    "eslint-plugin-storybook": "^0.5.12",
    "eslint-watch": "^7.0.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "npm-check-updates": "^10.0.0",
    "prettier": "^2.1.2",
    "storybook-addon-gatsby": "^0.0.5",
    "ts-jest": "^26.4.4",
    "typescript": "^4.0.5"
  }
}
